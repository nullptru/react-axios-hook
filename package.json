{
  "name": "react-axios-hook",
  "version": "2.0.1",
  "description": "axios hook for react",
  "main": "./dist/use-axios.common.js",
  "module": "./dist/use-axios.esm.js",
  "author": "nullptru@gmail.com",
  "license": "MIT",
  "typings": "./types",
  "files": [
    "dist",
    "types",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullptru/react-axios-hook.git"
  },
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=development rollup -c",
    "lint": "eslint src/**.{tsx,ts} --fix",
    "ci": "npx git-cz",
    "release": "standard-version",
    "trypublish": "npm publish || true",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "hooks",
    "axios",
    "typescript"
  ],
  "peerDependencies": {
    "axios": "^0.18.0 || ^0.19.0",
    "react": "^16.8.0-0"
  },
  "devDependencies": {
    "@babel/runtime": "7.7.4",
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "axios": "0.21.1",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rollup": "^1.27.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "standard-version": "7.0.1",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "src/**/*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
